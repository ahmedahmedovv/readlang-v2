{
  "manifest_version": 3,
  "name": "URL Logger",
  "version": "1.0",
  "description": "Shows current URL in console",
  "permissions": ["activeTab", "storage"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [".env"],
      "matches": ["<all_urls>"]
    }
  ]
} 